<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:CustomSlideOverMenu"
             x:Class="CustomSlideOverMenu.MainPage">

  <ContentPage.Content>
    <StackLayout>
      <Button Text="Left Slide Menu" Clicked="LeftSlide_OnClicked"></Button>
      <Button Text="Right Slide Menu"></Button>
      <Button Text="Top Slide Menu"></Button>
      <Button Text="Bottom Slide Menu"></Button>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>